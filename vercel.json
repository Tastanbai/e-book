{
    "version": 2,
    "builds": [
      {
        "src": "Dockerfile",
        "use": "@vercel/docker"
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles_build/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "myproject/wsgi.py"
      },
      {
        "src": "/(.*)",
        "dest": "myproject/wsgi.py"
      }
    ]
  }
  